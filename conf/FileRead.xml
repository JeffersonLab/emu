<component name="FileRead">
    <transports>
        <client name="BitBucket_SOCKET" class="CmsgSock"/>
        <client name="File" class="File" filename="dataFile.coda"/>
        <client name="File2" class="File"/>
    </transports>
    <modules src="modules.jar" usr_src="user_modules.jar">
        <ProcessModule class="Process">
            <inchannel name="Data File" transp="File"/>
            <inchannel name="Data File2" transp="File2"/>
            <outchannel name="SingleEmu" transp="BitBucket_SOCKET"/>
        </ProcessModule>
    </modules>
</component>

<?xml version="1.0"?>
<component>
    <name>EMU3</name>
    <transports>
        <EMU3_SOCKET type="SO" server="true" port="7030" size="200000" pool="2"/>
        <EMU4_SOCKET type="SO" server="false" port="7030" size="800000" pool="2"/>
        <EMU5_SOCKET type="SO" server="false" port="7030" size="800000" pool="2"/>
    </transports>
    <modules src="modules.jar" usr_src="user_modules.jar">
        <ReadModule type="Read">
            <ROC5 uses="EMU3_SOCKET"/>
            <ROC6 uses="EMU3_SOCKET"/>
            <ROC7 uses="EMU3_SOCKET"/>
            <ROC8 uses="EMU3_SOCKET"/>
        </ReadModule>
        <SendModule type="Send">
            <EMU3 uses="EMU4_SOCKET"/>
            <EMU3 uses="EMU5_SOCKET"/>
        </SendModule>
        <ProcessModule type="Process" concurrency="2">
            <ReadModule/>
            <SendModule output="true"/>
        </ProcessModule>
    </modules>
</component>
